// Time-shifting of a discrete time sequence

clear;close;clc;
x=input('Enter the squence:= ');
nx=input('Enter the time domain information of sequence:= ');
k=input('enter the shift value:= ');
nx1=nx+k;
nx2=nx-k;
subplot(3,1,1);
plot2d3(nx,x)
subplot(3,1,2);
plot2d3(nx1,x)
subplot(3,1,3);
plot2d3(nx2,x)
